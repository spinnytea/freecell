{
  "name": "freecell",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "prebuild": "npm run clean && npm run format && npm run lint && npm run test:cov",
    "build": "next build",
    "serve": "npx http-server -a localhost -p 8080 -o /freecell/manualtesting out",
    "fix": "npm run format && npm run lint",
    "lint": "next lint --fix",
    "format": "prettier --write --list-different .",
    "test": "jest --watch",
    "test:cov": "jest --coverage",
    "clean": "npx rimraf out && npx rimraf coverage && npm prune && git prune"
  },
  "dependencies": {
    "@gsap/react": "^2.1.1",
    "http-server": "^14.1.1",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.24.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/typescript-estree": "^7",
    "classnames": "^2.5.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "=4.7.4"
  }
}
